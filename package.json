{
  "name": "fitasafiddle",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "build:debug": "vite build --debug",
    "check:circular": "madge --circular src/",
    "check:circular-ts": "madge --circular --ts-config ./tsconfig.json --extensions ts,tsx src/",
    "check:deps": "madge --depends src/ --extensions js,jsx,ts,tsx",
    "check:all": "npm run check:circular && npm run check:deps",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
    "analyze": "npx vite-bundle-analyzer",
    "clean": "rm -rf dist node_modules/.vite",
    "rebuild": "npm run clean && npm install && npm run build",
    "serve": "vite preview --host 0.0.0.0 --port 4173"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@mui/material": "^5.15.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.0",
    "chart.js": "^4.4.0",
    "react-chartjs-2": "^5.2.0",
    "date-fns": "^2.30.0",
    "@netlify/functions": "^2.4.0",
    "pg": "^8.11.0",
    "@neondatabase/serverless": "^0.10.4",
    "bcryptjs": "^2.4.3",
    "lucide-react": "^0.263.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "vite": "^5.0.10",
    "madge": "^6.1.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-import": "^2.29.1",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "vite-bundle-analyzer": "^0.7.0",
    "circular-dependency-plugin": "^5.2.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "madge": {
    "detectiveOptions": {
      "es6": {
        "mixedImports": true
      },
      "ts": {
        "skipAsyncImports": true,
        "skipTypeImports": true
      },
      "tsx": {
        "skipAsyncImports": true,
        "skipTypeImports": true
      }
    }
  }
}
